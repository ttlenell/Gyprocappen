<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gyproc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/gyp_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/gyp_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".blager.BLagerAddActivity"></activity>
        <activity android:name=".blager.BLagerActivity" />
        <activity android:name=".logbook.LogbookAddActivity" />
        <activity android:name=".logbook.LogbookActivity" />
        <activity android:name=".messages.ChatWallActivity" />
        <activity android:name=".mainscreen.MainScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".messages.ChatLogActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".messages.MessagesActivity" />
        </activity>
        <activity android:name=".messages.NewMessageActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".messages.MessagesActivity" />
        </activity>
        <activity android:name=".messages.MessagesActivity" />
        <activity android:name=".registerlogin.RegisterActivity" />
        <activity android:name=".registerlogin.LoginActivity" />
    </application>

</manifest>